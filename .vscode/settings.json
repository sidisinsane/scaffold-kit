{
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.rulers": [
      {
        "column": 80,
        "color": "#ff9900"
      }
    ]
  },
  "[plaintext]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.rulers": [
      {
        "column": 80,
        "color": "#ff9900"
      }
    ]
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.rulers": [
      {
        "column": 80,
        "color": "#ff9900"
      }
    ]
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[yaml][yml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.rulers": [
      {
        "column": 80,
        "color": "#ff9900"
      }
    ]
  },
  "autoDocstring.docstringFormat": "google-notypes",
  "autoDocstring.generateDocstringOnEnter": true,
  "autoDocstring.guessTypes": false,
  "autoDocstring.includeExtendedSummary": true,
  "autoDocstring.includeName": false,
  "autoDocstring.logLevel": "Info",
  "autoDocstring.quoteStyle": "\"\"\"",
  "autoDocstring.startOnNewLine": false,
  "bandit.args": ["-c", "${workspaceFolder}/bandit.yaml"],
  "bandit.cwd": "${workspaceFolder}",
  "bandit.ignorePatterns": ["tests/**/*.py"],
  "codesnap.backgroundColor": "#abb8c3",
  "codesnap.realLineNumbers": true,
  "codesnap.roundedCorners": false,
  "codesnap.showLineNumbers": true,
  "codesnap.showWindowControls": false,
  "codesnap.showWindowTitle": true,
  "codesnap.shutterAction": "save",
  "codesnap.target": "container",
  "codesnap.transparentBackground": true,
  "debug.console.fontFamily": "Source Code Pro",
  "editor.autoIndent": "advanced",
  "editor.fontFamily": "Source Code Pro",
  "editor.fontSize": 12,
  "editor.fontWeight": "400",
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "editor.minimap.sectionHeaderFontSize": 9,
  "editor.quickSuggestionsDelay": 100,
  "editor.stickyScroll.enabled": true,
  "editor.tokenColorCustomizations": {
    "comments": "#ffee00"
  },
  "evenBetterToml.schema.enabled": true,
  "evenBetterToml.taplo.bundled": false,
  "evenBetterToml.taplo.configFile.enabled": true,
  "evenBetterToml.taplo.configFile.path": "${workspaceFolder}/taplo.toml",
  "evenBetterToml.taplo.path": "/opt/homebrew/bin/taplo",
  "explorer.compactFolders": false,
  "explorer.sortOrder": "default",
  "files.associations": {
    ".continueignore": "shellscript",
    ".coveragerc": "ini",
    ".pylintrc": "ini",
    ".gitignore": "shellscript",
    "*.env*": "dotenv",
    "LICENSE": "plaintext",
    "setup.cfg": "ini"
  },
  "files.encoding": "utf8",
  "files.eol": "auto",
  "files.exclude": {
    "*.lock": false,
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hatch": false,
    "**/.venv": false
  },
  "markdown-mermaid.lightModeTheme": "neutral",
  "markdown-mermaid.darkModeTheme": "neutral",
  "markdown-mermaid.languages": ["mermaid"],
  "markdownlint.configFile": "${workspaceFolder}/.markdownlint.yaml",
  "mypy-type-checker.args": [
    "--config-file=${workspaceFolder}/mypy.ini",
    "--install-types"
  ],
  "mypy-type-checker.preferDaemon": true,
  "mypy-type-checker.reportingScope": "workspace",
  "prettier.printWidth": 80,
  "prettier.proseWrap": "always",
  "prettier.singleQuote": false,
  "pylint.enabled": true,
  "pylint.interpreter": "${workspaceFolder}/.venv/bin/python",
  "pylint.path": "${workspaceFolder}/.venv/bin/pylint",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportArgumentType": "none",
    "reportMissingTypeStubs": "none",
    "reportMissingModuleSource": "none",
    "reportMissingImports": "none"
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
  "python.envFile": "${workspaceFolder}/.env",
  "python.languageServer": "Pylance",
  "python.terminal.activateEnvironment": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": false,
  "ruff.configuration": "${workspaceFolder}/ruff.toml",
  "ruff.interpreter": ["${workspaceFolder}/.venv/bin/python"],
  "ruff.nativeServer": true,
  "terminal.integrated.defaultProfile.osx": "bash",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "400",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${activeEditorShort}",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#ffee00",
    "tree.inactiveIndentGuidesStroke": "#5d5700"
  },
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": true,
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.indent": 12,
  "workbench.tree.renderIndentGuides": "always",
  "yaml.customTags": [
    "!Base64",
    "!Base64 mapping",
    "!Cidr sequence",
    "!And sequence",
    "!Equals sequence",
    "!ENV",
    "!ENV sequence",
    "!If sequence",
    "!Not sequence",
    "!Or sequence",
    "!Condition",
    "!FindInMap sequence",
    "!GetAtt",
    "!GetAtt sequence",
    "!GetAZs",
    "!ImportValue",
    "!Join sequence",
    "!Select sequence",
    "!Split sequence",
    "!Sub",
    "!Sub sequence",
    "!Transform mapping",
    "!Ref"
  ],
  "yaml.format.enable": true,
  "yaml.validate": false
}
